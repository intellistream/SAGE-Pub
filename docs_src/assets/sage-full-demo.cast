[18.589999999999957, "o", "\u001b[1;35mfrom sage.kernel import Pipeline\u001b[0m\r\n"]
[18.669999999999956, "o", "\u001b[1;35mfrom sage.libs.rag import DocumentLoader, VectorStore, Retriever\u001b[0m\r\n"]
[18.749999999999954, "o", "\u001b[1;35mfrom sage.libs.llm import LLMGenerator\u001b[0m\r\n"]
[18.809999999999953, "o", "\r\n"]
[18.86999999999995, "o", "\u001b[1;90m# Create RAG pipeline\u001b[0m\r\n"]
[18.94999999999995, "o", "\u001b[1;37mpipeline = Pipeline(\"rag-demo\")\u001b[0m\r\n"]
[19.00999999999995, "o", "\r\n"]
[19.069999999999947, "o", "\u001b[1;90m# Load documents\u001b[0m\r\n"]
[19.149999999999945, "o", "\u001b[1;37mdocs = DocumentLoader.from_text([\u001b[0m\r\n"]
[19.209999999999944, "o", "    \"SAGE is a streaming AI framework\",\r\n"]
[19.269999999999943, "o", "    \"It supports distributed LLM inference\",\r\n"]
[19.32999999999994, "o", "    \"Built for production-grade AI pipelines\"\r\n"]
[19.38999999999994, "o", "])\r\n"]
[19.44999999999994, "o", "\r\n"]
[19.509999999999938, "o", "\u001b[1;90m# Build vector index\u001b[0m\r\n"]
[19.589999999999936, "o", "\u001b[1;37mvector_store = VectorStore(embedding_model=\"bge-small\")\u001b[0m\r\n"]
[19.649999999999935, "o", "vector_store.add_documents(docs)\r\n"]
[19.709999999999933, "o", "\r\n"]
[19.769999999999932, "o", "\u001b[1;90m# Query pipeline\u001b[0m\r\n"]
[19.84999999999993, "o", "\u001b[1;37mquery = \"What is SAGE?\"\u001b[0m\r\n"]
[19.92999999999993, "o", "\u001b[1;37mretrieved = Retriever(vector_store, top_k=2).retrieve(query)\u001b[0m\r\n"]
[19.989999999999927, "o", "\r\n"]
[20.049999999999926, "o", "\u001b[1;90m# Generate response\u001b[0m\r\n"]
[20.129999999999924, "o", "\u001b[1;37mllm = LLMGenerator(model=\"Qwen2.5-7B\")\u001b[0m\r\n"]
[20.209999999999923, "o", "\u001b[1;37mresponse = llm.generate(query, context=retrieved)\u001b[0m\r\n"]
[20.26999999999992, "o", "\r\n"]
[20.32999999999992, "o", "print(f\"Query: {query}\")\r\n"]
[20.38999999999992, "o", "print(f\"Answer: {response}\")\r\n"]
[20.68999999999992, "o", "EOF\r\n"]
